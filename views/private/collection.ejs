<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Collections Report</h1>
        <ol class="breadcrumb">
            <li class="active">
                Period from <%=headers.start%> to <%=headers.end%>
            </li>
        </ol>
    </div>
    <div class='col-lg-12'>
        <div class="table-responsive">
            <table id="table-report-collection" class="table table-bordered table-hover table-striped" cellspacing="0" width="100%">
                <thead>
                    <tr>    
                        <th>Date</th>
                        <th>Name</th>
                        <th>Type</th>
                        <th>Amount</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (typeof report !== 'undefined') {
                        for (var i=0; i < report.length; i++) { %>
                        <tr>
                          <td><%=report[i].date %></td>
                          <td><%=report[i].name %></td>
                          <td><%=report[i].type %></td>
                          <td><%=report[i].amount %></td>
                        </tr>
                        <% }
                    }%>
                </tbody>
            </table>
        </div>
    </div>
</div>